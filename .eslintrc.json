{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  }
}
